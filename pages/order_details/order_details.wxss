.order_details{
    padding: 10rpx;
}
.order_details .service{
    font-size: 26rpx;
   vertical-align:middle;
  
}
.order_details .service .ser_icon {
   color: #6d6b6b;
}
.order_details .number{
    text-align: right;
}
 .cart_main_list .cart_item {
    display: flex;
    border-top: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
   margin-top: 20rpx;
}

 .cart_main_list .cart_item .cart_img_wrap {
    flex: 3;
    display: flex;
    justify-content: center;
    align-items: center;
}

 .cart_main_list .cart_item .cart_img_wrap image {
    width: 50%;
}

 .cart_main_list .cart_item .cart_info_wrap {
    flex: 4;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}
 .cart_main_list .cart_item .cart_info_wrap .goods_info{
    font-size: 25rpx;
    color: #6d6b6b;
    padding: 30rpx 0;
 }

 .cart_main_list .cart_item .cart_info_wrap .goods_name {
  font-weight: 500
}

 .cart_main_list .cart_item .cart_info_wrap .goods_time_wrap {
    display: flex;
    justify-content: space-between;
    vertical-align: middle;
    align-content: space-between;
}

 .cart_main_list .cart_item .cart_info_wrap .goods_time_wrap .goods_time {
    font-size: 28rpx;
    color: #6d6b6b
}
 .cart_main_list .cart_item .cart_info_wrap .goods_time_wrap .goods_pirce {
     font-size: 38rpx;
     color: orange;
 }




