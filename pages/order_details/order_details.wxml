<view class="first_tab">
    <Tabs bindtitleChange="handleTitleChange" tabs="{{tabs}}">
        <block wx:if="{{tabs[0].isShow}}">
            <!-- 预约信息 -->
            <view class="order_details">
                <van-row class="service">
                    <van-col span="12" class="ser_icon">
                        <van-icon name="notes-o" />
                        服务项目
                    </van-col>
                    <van-col class="number" span="12">span2122122128</van-col>
                </van-row>
                <serviceItem datas="{{datas}}"></serviceItem>
                <view class="info_content">
                    <view class="info_list">
                        <view class="info_title">状态</view>
                        <view class="info_name">待服务</view>
                    </view>
                    <view class="info_list">
                        <view class="info_title">购买数量</view>
                        <view class="info_name">1</view>
                    </view>
                    <view class="info_list">
                        <view class="info_title">服务人</view>
                        <view class="info_name">温秀先  1305433234222   本人</view>
                    </view>
                    <view class="info_list">
                        <view class="info_title">服务地址</view>
                        <view class="info_name">广州屌德斯时代大厦</view>
                    </view>
                    <view class="info_list">
                        <view class="info_title">服务时间</view>
                        <view class="info_name">201299-23</view>
                    </view>
                    <view class="info_list">
                        <view class="info_title">电子签名</view>
                        <view class="info_name">温秀先</view>
                    </view>
                    <view class="info_list" bindtap="onHistory">
                        <view class="info_title">患者病历</view>
                        <view class="info_name">5</view>
                        <text class="arrows">》</text>
                    </view>
                    <view class="info_list" bindtap="onExpense">
                        <view class="info_title">费用清单</view>
                        <view class="info_name  paid">
                            已支付
                            <text class="paid_price">￥ 119.00</text>
                            》
                        </view>
                    </view>
                    <view class="info_list">
                        <view class="info_title">备注</view>
                        <view class="info_name">疑问、疑问等</view>
                    </view>
                </view>
                <view class="service_btn">
                    <button class="btn" type="primary" bindtap="onStartService">开始服务</button>
                </view>
            </view>
        </block>
        <!-- 出门准备 -->
        <block wx:elif="{{tabs[1].isShow}}">
            <view class="goout_content">
                <view class="prompt">
                    <view class="prompt_title">温馨提示</view>
                    <view class="prompt_list">
                        <view>1、出门前请务必携带安全设备装置</view>
                        <view>2、请务必携带护理工具包</view>
                    </view>
                </view>
                <view class="card  prompt">
                    <view class="card_info">
                        <view class="card_time">
                            <van-icon name="underway-o" class="card_icon" />
                            出门打卡
                            <text>8:00</text>
                            <text class="itme_bin">已打卡</text>
                        </view>
                        <view class="card_address">
                            <van-icon name="location-o" class="card_icon" />
                            广东省妇幼医院d栋
                        </view>
                        <view class="card_img">
                            <van-icon name="location-o" class="card_icon" />
                            <image src="../../img/wechat.png"></image>
                        </view>
                    </view>
                    <view class="sign">
                        <view class="qiandao">
                            <image src="../../img/qiandao.png"></image>
                        </view>
                    </view>
                </view>
                <button type="primary" bindtap="onPrepare" class="btn">准备完成</button>
            </view>
        </block>
        <!-- 到达地点 -->
        <block wx:elif="{{tabs[2].isShow}}">
            <view class="arrive_content  goout_content">
                <view class="card  prompt">
                    <view class="card_info">
                        <view class="card_time">
                            <van-icon name="underway-o" class="card_icon" />
                            出门打卡
                            <text>8:00</text>
                            <text class="itme_bin">已打卡</text>
                        </view>
                        <view class="card_address">
                            <van-icon name="location-o" class="card_icon" />
                            广东省妇幼医院d栋
                        </view>
                        <view class="card_img">
                            <van-icon name="location-o" class="card_icon" />
                            <image src="../../img/wechat.png"></image>
                        </view>
                    </view>
                    <view class="sign">
                        <view class="qiandao">
                            <image src="../../img/qiandao.png"></image>
                        </view>
                    </view>
                </view>
                <view class="prompt">
                    <view class="realNames">
                        <view class="shim">实名认证</view>
                        <view class="weiren">未认证</view>
                    </view>
                    <view class="info_input">
                        <van-cell-group>
                            <van-field value="{{ value }}" label="患者姓名:" placeholder="请输入患者姓名" border="{{ false }}" bind:change="onChange" />
                            <van-field value="{{ value }}" label="身份证号码:" placeholder="请输入患者姓身份证号码" border="{{ false }}" bind:change="onChange" />
                        </van-cell-group>
                    </view>
                    <view class="photos">
                        <view class="photos_position">
                            <image src="../../img/wechat.png"></image>
                        </view>
                        <view class="photos_btn">
                            <image src="../../img/photos.jpg"></image>
                        </view>
                    </view>
                </view>
                <view class="arrive_btn">
                    <button type="primary" bindtap="onOderderOff">取消订单</button>
                    <button type="primary" bindtap="onNextStep">下一步</button>
                </view>
            </view>
        </block>
        <!-- 评估报告 -->
        <block wx:elif="{{tabs[3].isShow}}">
            <view class="prompt">
                <view class="assess_list" wx:for="{{items}}" wx:key="{{value}}">
                    <view class="assess_title">{{item.title}}</view>
                    <view class="assess_checked">
                        <checkbox-group bindchange="checkboxChange">
                            <label class="checkbox">
                                <checkbox value="{{item.name}}" checked="{{item.checked}}" />
                                {{item.value}}
                            </label>
                                                         <label class="checkbox">
                                 <checkbox value="{{item.name}}" checked="{{item.checked}}" wx:if="{{item.value1}}"/>
                                 {{item.value1}}
                             </label>
                        </checkbox-group>
                    </view>
                </view>
            </view>
        </block>
        <!-- 护理 -->
        <block wx:elif="{{tabs[4].isShow}}">护理-正在开发中。。。。</block>
        <!-- 安全打卡 -->
        <block wx:elif="{{tabs[5].isShow}}">安全打卡-正在开发中。。。。</block>
    </Tabs>
</view>